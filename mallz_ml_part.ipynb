    "import easyocr\n",
    "import tkinter as tk\n",
    "from tkinter import messagebox\n",
    "import cv2\n",
    "import numpy as py \n",
    "import time\n",
    "import regex as re\n",
    "from pymongo import MongoClient\n",
    "from datetime import datetime\n",
    "\n",
    "\n",
    "\n",
    "operating_mode=\"NA\"\n",
    "\n",
    "def option_selected(option):\n",
    "    messagebox.showinfo(\"Selection\", f\"You selected: {option}\")\n",
    "    global operating_mode\n",
    "    operating_mode=option\n",
    "    root.destroy()\n",
    "\n",
    "# Create the main window\n",
    "root = tk.Tk()\n",
    "root.title(\"Choose an Option\")\n",
    "\n",
    "# Create a label to prompt the user\n",
    "label = tk.Label(root, text=\"Please choose an option:\")\n",
    "label.pack(pady=10)\n",
    "\n",
    "# Create two buttons for the options\n",
    "button1 = tk.Button(root, text=\"Entry Mode\", command=lambda: option_selected(\"Entry Mode\"))\n",
    "button1.pack(pady=5)\n",
    "\n",
    "button2 = tk.Button(root, text=\"Exit Mode\", command=lambda: option_selected(\"Exit Mode\"))\n",
    "button2.pack(pady=5)\n",
    "\n",
    "# Start the Tkinter event loop\n",
    "root.mainloop()\n",
    "\n",
    "def connect_to_mongo():\n",
    "\n",
    "    client = MongoClient(\"mongoapi here")\n",
    "    return client.mallEZ1\n",
    "\n",
    "def add_parking_record(db, numberplate: str, date_and_time: str):\n",
    "    parking_collection = db.Parking\n",
    "    record = {\n",
    "        'numberplate': numberplate,\n",
    "        'date_and_time': date_and_time,\n",
    "    }\n",
    "    # Insert the record into the collection\n",
    "    result = parking_collection.insert_one(record)\n",
    "    print(f'Record added with id: {result.inserted_id}')\n",
    "\n",
    "def print_all_parking_records(db):\n",
    "    parking_collection = db.Parking\n",
    "    records = parking_collection.find()  # Retrieve all records\n",
    "\n",
    "    for record in records:\n",
    "        print(record)\n",
    "\n",
    "def delete_record_by_numberplate(db, numberplate: str):\n",
    "    parking_collection = db.Parking\n",
    "    # Find the record by number plate\n",
    "    record = parking_collection.find_one({'numberplate': numberplate})\n",
    "    if record:\n",
    "        # Calculate parking duration\n",
    "        parked_time = datetime.fromisoformat(record['date_and_time'])\n",
    "        current_time = datetime.now()\n",
    "        duration_minutes = (current_time - parked_time).total_seconds() / 60\n",
    "        # Delete the record\n",
    "        parking_collection.delete_one({'numberplate': numberplate})\n",
    "        print(f'Record for {numberplate} deleted. It was parked for {duration_minutes:.2f} minutes.')\n",
    "        return duration_minutes*0.5\n",
    "    \n",
    "    print(f'Record with number plate {numberplate} not found.')\n",
    "    return 0\n",
    "\n",
    "def get_numberplate(img_path):\n",
    "    reader = easyocr.Reader(['en'])\n",
    "    result = reader.readtext(img_path)\n",
    "    return result[0][1]\n",
    "\n",
    "def numberplate_validator(raw_text):\n",
    "    pattern = r'\\b[A-Z]{2}[-.\\s]?\\d{2}[-.\\s]?[A-Z]{1,2}[-.\\s]?\\d{4}\\b'\n",
    "    regex = re.compile(pattern)\n",
    "    matches = regex.findall(raw_text)\n",
    "    return matches if matches!=[] else \"NULL\"\n",
    "\n",
    "def add_to_queue(numberplate):\n",
    "    \n",
    "    current_datetime = datetime.now()  # Get current date and time\n",
    "    datetime_str = current_datetime.isoformat()\n",
    "    add_parking_record(mongo_client,numberplate,datetime_str)\n",
    "    \n",
    "def remove_from_queue(numberplate):\n",
    "    cost=delete_record_by_numberplate(mongo_client,numberplate)\n",
    "    if cost!=0:\n",
    "        return cost\n",
    "    return \"error car not found\"\n",
    "\n",
    "mongo_client=connect_to_mongo()# first try connecting\n",
    "current_number_plate=\"\"\n",
    "cap = cv2.VideoCapture(0)\n",
    "if not cap.isOpened():\n",
    "    print(\"Error: Could not open video stream.\")\n",
    "    exit()\n",
    "def capture_image(frame, filename=\"image2.png\"):\n",
    "    cv2.imwrite(filename, frame)\n",
    "try:\n",
    "    while True:\n",
    "        ret, frame = cap.read()\n",
    "        if not ret:\n",
    "            break\n",
    "        capture_image(frame)\n",
    "        current_number_plate=get_numberplate(\"image2.png\")\n",
    "        if operating_mode==\"Entry Mode\":\n",
    "            add_to_queue(current_number_plate)\n",
    "        elif operating_mode==\"Exit Mode\":\n",
    "            remove_from_queue(current_number_plate)\n",
    "        else:\n",
    "            pass\n",
    "        time.sleep(6)\n",
    "finally:\n",
    "\n",
    "    cap.release()\n",
    "    cv2.destroyAllWindows()"
   ]
  },
