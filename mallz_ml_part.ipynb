    "import cv2\n",
    "import time\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "if not cap.isOpened():\n",
    "    print(\"Error: Could not open video stream.\")\n",
    "    exit()\n",
    "def capture_image(frame, filename=\"image2.png\"):\n",
    "    cv2.imwrite(filename, frame)\n",
    "    print(f\"Image saved as {filename}\")\n",
    "try:\n",
    "    while True:\n",
    "        ret, frame = cap.read(\n",
    "        if not ret:\n",
    "            break\n",
    "        capture_image(frame)\n",
    "        time.sleep(6)\n",
    "\n",
    "finally:\n",
    "    # Release the video stream when done\n",
    "    cap.release()\n",
    "    cv2.destroyAllWindows()\n"
   ]
